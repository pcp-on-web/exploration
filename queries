-sparql "select ?s ?slabel where { ?s <http://www.w3.org/2000/01/rdf-schema#label> ?slabel. FILTER (CONTAINS(?slabel, 'mense')) }" 
-kb "http://uni-helmstedt.hab.de/cph" -install > contains_mense_result.txt


